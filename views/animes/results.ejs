<h1>Working</h1>
<% if (results) {%> 
  <main>
    <% results.forEach(anime => { %>
      <div class="card-body">
        <p class="card-ttle"><%= anime.titles.en %> </p>
        <p class="card-ttle"><%= anime.titles.jp %> </p>
      </div>

    <% }) %>
  </main>
<% } %> 
